{
  "name": "runcellswithtag",
  "displayName": "RunCellsWithTag",
  "description": "Run cells with the specified tag.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "icon": "images/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "runcellswithtag.runCellsWithTag",
        "title": "Run Cells With Tag"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "keywords": ["vscode", "jupyter", "notebook", "cell", "tag", "run"],
  "repository": {
    "type": "git",
    "url": "https://github.com/jantomec/RunCellsWithTag.git"
  },
  "homepage": "https://github.com/jantomec/RunCellsWithTag#readme",
  "license": "MIT"
}
